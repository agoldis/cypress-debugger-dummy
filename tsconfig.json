{
  "compilerOptions": {
    "importHelpers": true,
    "sourceMap": true,
    "declaration": false,
    "esModuleInterop": true,
    "moduleResolution": "node16",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "target": "es2015",
    "module": "es2020",
    "baseUrl": ".",
    "rootDir": ".",
    "types": [
      "cypress",
      "node",
      "cypress-fill-command",
      "cypress-wait-until",
      "cypress-real-events",
      "node_modules/@types"
    ],
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2018", "dom"],
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["node_modules"],
  "include": ["cypress/**/*.ts"]
}
